# General
PORT=5000

# OpenAI Configuration (use API key from same project as your Playground agent)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_REALTIME_MODEL=gpt-realtime

# Playground Realtime agent – right sidebar "Prompt" field (full ID) and version dropdown (v1 → "1", v2 default → "2")
OPENAI_PROMPT_ID=pmpt_69948eeec39481959a7cc4250542590f01fd5d101c307675
OPENAI_PROMPT_VERSION=2

# Twilio Configuration (Required for Voice functionality)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Database (Optional, defaults to MemStorage if not used for persistence)
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname
